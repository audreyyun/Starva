<!DOCTYPE html>
<html>
  <head>
    <title>Starva</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <script type="text/javascript">
      window.sessionbg = "<%= image_url('christian.png') %>"
    </script>
    
    <script type="text/javascript">
      window.starvalogo = "<%= image_url('starva-logo.png') %>"
    </script>
    <script type="text/javascript">
      window.splashpage = "<%= image_url('splash-page.png') %>"
    </script>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
